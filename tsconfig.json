{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@domain/*": ["src/domain/*"],
      "@features/*": ["src/features/*"],
      "@functions/*": ["src/functions/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
